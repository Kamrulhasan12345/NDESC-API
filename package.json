{
	"name": "ndesc",
	"version": "0.0.1",
	"description": "The internal API for Notre Dame Earth and Science Club official website",
	"type": "module",
	"main": "src/server.js",
	"scripts": {
		"start": "",
		"dev": "node -r dotenv/config --experimental-loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
