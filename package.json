		"@tinyhttp/rate-limit": "^2.0.4",
		"@tinyhttp/router": "^2.0.4",
		"@types/bcrypt": "^5.0.0",
		"@types/nodemailer": "^6.4.4",
		"@types/uuid": "^8.3.3",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.0",
		"dotenv": "^10.0.0",
		"firebase-admin": "^10.0.0",
		"nodemailer": "^6.7.2",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/sinon": "^10.0.6",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
